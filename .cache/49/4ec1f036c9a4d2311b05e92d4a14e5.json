ted":{"line":111,"column":18},"source":"src/pages/Pets.js","original":{"line":65,"column":4}},{"generated":{"line":111,"column":20},"source":"src/pages/Pets.js","original":{"line":77,"column":6}},{"generated":{"line":111,"column":66},"source":"src/pages/Pets.js","original":{"line":78,"column":8}},{"generated":{"line":111,"column":95},"source":"src/pages/Pets.js","original":{"line":78,"column":9}},{"generated":{"line":111,"column":112},"source":"src/pages/Pets.js","original":{"line":78,"column":8}},{"generated":{"line":112,"column":0},"source":"src/pages/Pets.js","original":{"line":78,"column":18}},{"generated":{"line":112,"column":4},"source":"src/pages/Pets.js","original":{"line":78,"column":18}},{"generated":{"line":112,"column":8},"source":"src/pages/Pets.js","original":{"line":78,"column":22}},{"name":"data","generated":{"line":112,"column":10},"source":"src/pages/Pets.js","original":{"line":78,"column":24}},{"generated":{"line":112,"column":14},"source":"src/pages/Pets.js","original":{"line":78,"column":28}},{"name":"pets","generated":{"line":112,"column":15},"source":"src/pages/Pets.js","original":{"line":78,"column":29}},{"generated":{"line":113,"column":0},"source":"src/pages/Pets.js","original":{"line":78,"column":8}},{"generated":{"line":113,"column":4},"source":"src/pages/Pets.js","original":{"line":77,"column":6}},{"generated":{"line":113,"column":5},"source":"src/pages/Pets.js","original":{"line":65,"column":4}},{"generated":{"line":113,"column":6},"source":"src/pages/Pets.js","original":{"line":64,"column":2}},{"generated":{"line":114,"column":0},"source":"src/pages/Pets.js","original":{"line":82,"column":1}}],"sources":{"src/pages/Pets.js":"import React, {useState} from 'react'\nimport gql from 'graphql-tag'\nimport { useQuery, useMutation } from '@apollo/react-hooks'\nimport PetsList from '../components/PetsList'\nimport NewPetModal from '../components/NewPetModal'\nimport Loader from '../components/Loader'\nimport { unstable_batchedUpdates } from 'react-dom'\n\nconst query = gql`\n  query AllPets{\n    pets{\n      id\n      name \n      type\n    }\n  }\n`\n\nconst NEW_PET = gql`\n  mutation CreateAPet($newPet: newPetInput!){\n    addPet: newPet(input: $newPet){\n      id\n      name\n      type\n    }\n  }\n`\nexport default function Pets () {\n  const [modal, setModal] = useState(false)\n  const {data , loading , error} = useQuery(query)\n  const [createPet , newPet] = useMutation(NEW_PET,{\n  update(cache,{data:{addPet}}){\n    console.log(\"inside of update\")\n    const allPets = cache.readQuery({query: query})\n    cache.writeQuery({\n      query: query,\n      data:{pets:[addPet,...allPets.pets]}\n    })\n  }\n});\n  const onSubmit = input => {\n    console.log(input)\n    setModal(false);\n    createPet({\n      variables:{newPet:input}\n    }); \n  }\n  \n  if(loading || newPet.loading)\n  {\n    return <Loader />    //since useQuery is async in background\n  }\nif(error || newPet.error)\n  {\n    return <p>error</p>\n  }\n\n  if(error)\n  console.log(data)\n  if (modal) {\n    return <NewPetModal onSubmit={onSubmit} onCancel={() => setModal(false)} />\n  }\n\n  return (\n    <div className=\"page pets-page\">\n      <section>\n        <div className=\"row betwee-xs middle-xs\">\n          <div className=\"col-xs-10\">\n            <h1>Pets</h1>\n          </div>\n\n          <div className=\"col-xs-2\">\n            <button onClick={() => setModal(true)}>new pet</button>\n          </div>\n        </div>\n      </section>\n      <section>\n        <PetsList pets={data.pets}/>\n      </section>\n    </div>\n  )\n}\n"},"lineCount":null}},"error":null,"hash":"960b9ec60e4631e70c58293f26edfe30","cacheData":{"env":{}}}